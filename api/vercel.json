{
  "version": 2,
  "builds": [
    {
      "src": "/api/update-password.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/search-user.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/recent-search.js",
      "use": "@vercel/node"
    },
     {
      "src": "/api/add-story.js",
      "use": "@vercel/node"
    },
     {
      "src": "/api/getAll-story.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/view-story.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/get-mystory.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/update-password",
      "dest": "api/update-password.js",
      "timeout": 60
    },
     {
      "src": "/api/search-user",
      "dest": "api/search-user.js",
      "timeout": 60
    },
     {
      "src": "/api/recent-search",
      "dest": "api/recent-search.js",
      "timeout": 60
    },
     {
      "src": "/api/add-story",
      "dest": "api/add-story.js",
      "timeout": 60
     },
     {
      "src": "/api/getAll-story",
      "dest": "api/getAll-story.js",
      "timeout": 60
     },
     {
      "src": "/api/view-story",
      "dest": "api/view-story.js",
      "timeout": 60
     },
     {
      "src": "/api/get-mystory",
      "dest": "api/get-mystory.js",
      "timeout": 60
     }
  ]
}
